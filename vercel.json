{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "VERCEL_ENV": "production",
    "WTF_CSRF_CHECK_DEFAULT": "false"
  }
} 